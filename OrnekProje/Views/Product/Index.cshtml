<h3>viewbag</h3>
<ul>
    @foreach (var product in ViewBag.products as List<OrnekProje.Model.Product>) {
        <li>@product.ProductName</li>
    }
</ul>

<h3>viewdata</h3>
<ul>
    @foreach (var product in ViewData ["urunler"] as List<OrnekProje.Model.Product>) {
        <li>@product.ProductName</li>
}
</ul>